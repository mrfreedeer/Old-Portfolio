;RIGHT?
(define (right? x  z number)
  (cond ((or (= number 1)
             (= number 2)
             (= number 2.1)
             (= number 2.2)
             (= number 3)
             (= number 4)
             (= number 5)
             (= number 6.3)
             (= number 7.3)
             (= number 4.1)
             )
             (if (>= (+ x 44) z) #t
                          #f)
                      )
     
        ((or (= number 2.3)
             (= number 3.1)
             (= number 5.1)
             (= number 6.1)
             (= number 6.2)
             (= number 7)
             (= number 7.1)
             )
             (if (>= (+ x 22) z) #t
                          #f)
                      )
        
        ((or (= number 6) (= number 7.2)) (if (>= (+ x 66) z) #t
                          #f)
                      )
       
        ((= number 7.3) (if (>= (+ x 44) z) #t
                          #f)
                      )
        )
  )
(define (oimate? x y1 y2 z number)
  (cond ((or (= number 1)
             (= number 2)
             (= number 2.1)
             (= number 2.2)
             (= number 3)
             (= number 4)
             (= number 5)
             (= number 6.3)
             (= number 7.3)
             (= number 4.1)
             )
             (if (and (>= y1 y2) (>= (+ x 44) z)) #t
                          #f)
                      )
     
        ((or (= number 2.3)
             (= number 3.1)
             (= number 5.1)
             (= number 6.1)
             (= number 6.2)
             (= number 7)
             (= number 7.1)
             )
             (if (and (>= y1 y2) (>= (+ x 22) z)) #t
                          #f)
                      )
        
        ((or (= number 6) (= number 7.2)) (if (and (>= y1 y2) (>= (+ x 66) z)) #t
                          #f)
                      )
       
        ((= number 7.3) (if (and (>= (+ x 44) z) (>= y1 y2)) #t
                          #f)
                      )
        )
  )
(define (over x y vec dindex number)
  (if (equal? (vector-ref vec 0) 0) #f
  (if(>= (- dindex 1) 0)
     (if (oimate? x y (vector-ref (vector-ref vec (- dindex 1)) 1) (vector-ref (vector-ref vec (- dindex 1)) 0) number)
         #t
         (over x y vec (- dindex 1) number)
         )
     #f
     )
  )
  )
        
;LEFT
(define (left? x  z number)
  (cond ((or (= number 1)
             (= number 2.1)
             (= number 3.1)
             (= number 6)
             (= number 4.1)
             (= number 6.3)
             (= number 7.2)
             (= number 7.3)
             )
             (if (<= x z) #t
                          #f
                          )
                      )
        ((or (= number 2)
             (= number 2.2)
             (= number 2.3)
             (= number 4)
             
              (= number 5)
             (= number 5.1)
             (= number 6.1)
             (= number 7.1)
             )
         (if (<= (- x 22) z) #t
                          #f
                          )
                      )
        ((or (= number 6.2)
             (= number 7)
             (= number 3)
             )
         (if (<= (- x 44) z) #t
             #f
             )
         )
        )
  )

 (define (maps? x y1 y2 z number)
  (cond ((or (= number 1)
             (= number 2.1)
             (= number 3.1)
             (= number 5)
             (= number 6)
             (= number 6.3)
             (= number 7.2)
             (= number 7.3)
             )
             (if (and (>= y1 y2) (<= x z)) #t
                          #f)
                      )
     
        ((or (= number 2)
             (= number 2.2)
             (= number 2.3)
             (= number 4)
             (= number 4.1)
             (= number 5.1)
             (= number 6.1)
             (= number 7.1)
             )
             (if (and (>= y1 y2) (<= (- x 22) z)) #t
                          #f)
                      )
        
        ((or (= number 6.2)
             (= number 7)
             ) (if (and (>= y1 y2) (<= (- x 44) z)) #t
                          #f)
                      )
         
       
        
        )
  )
(define (under x y vec dindex number)
  (if (equal? (vector-ref vec 0) 0) #f
  (if(>= (- dindex 1) 0)
     (if (maps? x y (vector-ref (vector-ref vec (- dindex 1)) 1) (vector-ref (vector-ref vec (- dindex 1)) 0) number)
         #t
         (under x y vec (- dindex 1) number)
         )
     #f
     )
  )
  ) 

